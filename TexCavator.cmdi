         <GeneralInfo>
            <name xml:lang="eng">Texcavator</name>
            <title xml:lang="eng"/>
            <publicationYear>2015</publicationYear>
            <url>http://texcavator.surfsaralabs.nl/</url><CLARINCentre>Huygens ING</CLARINCentre>
			<TimeCoverage>
			  <startYear>1850</startYear>
