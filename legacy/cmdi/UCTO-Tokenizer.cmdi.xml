            <name xml:lang="eng">Ucto</name>
            <title xml:lang="eng">Ucto Tokeniser</title>
			<version>v0.13</version>
            <publicationYear>2011-03-27</publicationYear>
            <url>https://webservices-lst.science.ru.nl/ucto/</url><CLARINCentre>none yet</CLARINCentre>
            <ReleaseStatus>
